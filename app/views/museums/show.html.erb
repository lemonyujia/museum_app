<h1><%= @museum.name %></h1>
<p><%= %></p>

<hr>

<% @museum.paintings.each do |painting| %>
  <section>
    <h3><strong>Title</strong>: <%= link_to painting.title, painting %></h3>
    <p><strong>Year</strong>: <%= link_to painting.year %></p>
    <p><strong>Style</strong>: <%= painting.style %></p>
    <hr>
  </section>
<% end %>

<p><%= link_to "Edit Museum", edit_museum_path(@museum) %></p>